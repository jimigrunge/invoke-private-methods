{
  "name": "jimigrunge/invoke-private-methods",
  "description": "This is a class to aid in unit testing of private methods.",
  "type": "library",
  "keywords": [
    "phpunit",
    "phpspec",
    "testing",
    "helper",
    "private",
    "method"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "James Grundner",
      "email": "james@jgrundner.com",
      "homepage": "http://jgrundner.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jimigrunge\\InvokePrivateMethods\\": "src/"
    }
  },
  "require": {
    "php": ">=5.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || 5.*",
    "squizlabs/php_codesniffer": "~2.5",
    "sebastian/phpcpd": "~2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Jimigrunge\\InvokePrivateMethods\\Tests\\": "tests/"
    }
  }
}
